{
  "name": "linksapp",
  "version": "1.1.2",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "HOST=0.0.0.0 PORT=3000 rollup -c -w",
    "start": "rollup -c && sirv public --no-clear",
    "start:vercel": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier -w .",
    "check-format": "prettier -c ."
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@tsconfig/svelte": "^5.0.0",
    "prettier": "3.0.0",
    "rollup": "3.27.0",
    "rollup-plugin-css-only": "^4.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "4.1.2",
    "svelte-check": "3.4.6",
    "svelte-preprocess": "5.0.4",
    "tslib": "2.6.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "jquery": "^3.7.0",
    "js-cookie": "^3.0.5",
    "sass": "1.64.2",
    "sirv-cli": "2.0.2",
    "svelte-fa": "^3.0.2",
    "svelte-seo": "1.5.3",
    "sveltestrap": "5.10.0"
  },
  "repository": {
    "url": "https://github.com/sebastianjnuwu/LinksApp.git"
  },
  "bugs": {
    "url": "https://github.com/sebastianjnuwu/LinksApp/issues"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
